<h1 class="page-header">Here are <%= @user.first_name %>'s current matches:</h1>





<% if @users.length().>(1) %>
  <% @users.each() do |user2| %>
    <% if @user.preferred_match(user2) == 3 && user2.preferred_match(@user) == 3 %>
      <a href="/users/<%= user2.id() %>/profile"><%= user2.first_name() %> <%= user2.last_name() %></a>
      <br>
    <% end %>
    <% end %>
    <% else %>
    <h3>There are no matches.</h3>
  <% end %>
